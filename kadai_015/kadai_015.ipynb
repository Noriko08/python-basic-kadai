        "class Human:\n",
        "    def __init__(self,name,age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(self.name)\n",
        "        print(self.age)\n",
        "\n",
        "# インスタンス化する\n",
        "taro = Human(\"太郎\",13)\n",
        "\n",
        "# メソッドにアクセスして実行する\n",
        "taro.printinfo()"
